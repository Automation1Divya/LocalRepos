<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sample Suite">
	<test name="testing">
		<groups>
			<run>
				<exclude name="Sanity Test" />
				<include name="Regression" />
				<include name="Smoke Test" />

			</run>
		</groups>
		<classes>
			<class name="advancetestng.GroupExamples" />
			<class name="advancetestng.GroupExample2" />
		</classes>
	</test>
</suite>
